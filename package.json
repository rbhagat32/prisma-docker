{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "description": "This app was created using npx create-types-backend@latest",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "npm run lint && tsc && tsc-alias",
    "dev": "nodemon",
    "format": "npx prettier --write .",
    "lint": "eslint src",
    "docker-up:prod": "docker compose -f compose.prod.yaml up -d --build",
    "docker-up:dev": "docker compose -f compose.dev.yaml up --watch",
    "docker-down:prod": "docker compose -f compose.prod.yaml down --rmi local --remove-orphans && docker image prune -f",
    "docker-down:dev": "docker compose -f compose.dev.yaml down --rmi local --remove-orphans && docker image prune -f"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.1",
    "prisma": "^7.0.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  }
}
